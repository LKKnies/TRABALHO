CREATE TABLE CARROS (
                    NOME VARCHAR(50) PRIMARY KEY,  
                    MARCA VARCHAR(50),  
                    ANO INT,
                    COR VARCHAR(50),
                    VALOR VARCHAR(50))

INSERT INTO CARROS (NOME, MARCA, ANO, COR) VALUES ('CORSA', 'CHEVROLET', 2010, 'AZUL' )

UPDATE CARROS SET VALOR = '25000' WHERE NOME = 'CORSA'

SELECT * FROM CARROS

DELETE FROM CARROS WHERE NOME = 'CORSA'
